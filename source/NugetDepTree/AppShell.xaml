<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="NugetDepTree.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:NugetDepTree.Views"
	xmlns:local="clr-namespace:NugetDepTree">

	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Main"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:MainPage}"
		Route="MainPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Localization"
		Icon="iconlocalization.png"
		ContentTemplate="{DataTemplate views:LocalizationPage}"
		Route="LocalizationPage" />
</Shell>
